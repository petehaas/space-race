capsule {
  id (playground.spacex)
  version (0.1.0)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  runtime-version (6)
  store-countries {
    all
  }

  store-sections {
    section (EducationAndReference)
  }
  device-models {
    all
  }

  default-action (GetMenu)

  permissions {
    bixby-user-id-access
  }

 capsule-imports {
   import (viv.time) {
     as (time) version (3.1.115)
   }
   import (viv.geo) {
     as (geo) version (9.26.22)
   }
 }
}
